@{
}

@model ProjectsViewModel
<h1>HEllo!</h1>

@foreach (Project project in Model.Projects)
    {
        <h3>@project.ProjectName</h3>


        <ul>
            <li> Regional Program: @project.ProjectRegionalProgram </li>
            <li> Project Impact: @project.ProjectImpact indivduals served.</li>
            <li> Project Type: @project.ProjectType</li>
            <li> Project Phase: @project.ProjectPhase</li>
            <li>Status: @project.ProjectFunctionalityStatus</li>
        </ul>
    }



<div page-model="@Model.PageInfo" page-action="Index"></div>